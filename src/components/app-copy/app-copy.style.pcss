.app-copy {
  padding: 40px;
  background-color: var(--gray--900);
  border-top: 4px solid var(--amber--900);
  display: flex;
  justify-content: space-between;
  color: var(--white);
  & a {
    transition-duration: 0.3s;
    color: var(--white);
    text-decoration: none;
    &:hover {
      color: var(--amber--900);
    }
  }
}
